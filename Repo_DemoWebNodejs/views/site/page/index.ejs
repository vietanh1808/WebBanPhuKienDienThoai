<% include ../layout/header.ejs %>
<!-- Header End -->

<div id="maincontainer">
  <!-- Slider Start-->
  <div id="myCarousel" class="carousel slide">
    <!-- Carousel items -->
    <div class="carousel-inner">
      <div class="active item"><a href="/cate/asus.60d31b8382d8b325b4153465.html"><img src="img/banner1111.jpg" alt="" width="100%" height="100px" /></a></div>
      <div class="item"><a href="/chi-tiet/huawei-p30-lite.60d324bb82d8b325b4153566.60d31ca982d8b325b41534b5.html"><img src="img/banner2222.jpg" alt="" width="100%" height="100px" /></a></div>
      <div class="item"><a href="/chi-tiet/iphone-7-plus-32gb.60d3181682d8b325b41533fc.606dd04f596dc2074453c604.html"><img src="img/banner1.jpg" alt="/chi-tiet/iphone-7-plus-32gb.60d3181682d8b325b41533fc.606dd04f596dc2074453c604.html" width="100%" height="100px" /></a></div>
      <div class="item"><img src="img/banner2.jpg" alt="" width="100%" height="100px" /></div>
    </div>
    <!-- Carousel nav -->
    <a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
    <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
  </div>
  <!-- Slider End-->
  
  <!-- Section Start-->
  <section class="container otherddetails">
    <div class="otherddetailspart">
      <div class="innerclass free">
        <h2>Miễn phí Ship</h2>
        Cho tất cả các đơn hàng trên 200k </div>
    </div>
    <div class="otherddetailspart">
      <div class="innerclass payment">
        <h2>Thanh Toán Đơn Giản</h2>
       hỗ trợ thanh toán đa phương thức </div>
    </div>
    <div class="otherddetailspart">
      <div class="innerclass shipping">
        <h2>Vận Chuyển Nhanh Chóng</h2>
       hệ thống đội ngũ giao hàng tốc độ </div>
    </div>
    <div class="otherddetailspart">
      <div class="innerclass choice">
        <h2>Hơn 5000 Lựa Chọn</h2>
        50,000 sản phẩm </div>
    </div>
  </section>
  <!-- Section End-->
  
  <!-- Featured Product-->
   <%   cate.forEach(function(ca){ var id = 0; %>
      <section id="featured" class="row mt40">
        <div class="container">
          <h1 class="heading1"><span class="maintext"> <%= ca.name%> </span><span class="subtext"></span></h1>
          <ul class="thumbnails">
            <%  product.forEach(function(pro){ if(id < product.length) { if(pro.cateId == ca._id){ id ++ ; %>

              <li class="span3">
                <a class="prdocutname" href="product.html"> <%= pro.name %> </a>
                <div class="thumbnail">
                  <span class="sale tooltip-test">Sale</span>
                  <a href="/chi-tiet/<%= pro.nameKhongDau%>.<%= pro._id%>.<%= pro.cateId%>.html"><img alt="" src="upload/<%= pro.img %>"></a>
                  <div class="pricetag">
                    <span class="spiral"></span><a href="/add-cart.<%= pro._id %>" class="productcart">Thêm Vào Giỏ</a>
                    <div class="price">
                      <div class="pricenew"><%= pro.price %></div>
                    </div>
                  </div>
                </div>
              </li>

            <% }}})%>
          </ul>
        </div>
      </section>
  <% })%>
  <!-- Latest Product-->

<!-- Footer -->
<% include ../layout/footer.ejs %>